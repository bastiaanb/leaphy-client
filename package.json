{
  "name": "easybloqs",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean:darwin": "rm -f easybloqs.dmg && rm -rf dist && rm -rf easybloqs-darwin-x64",
    "clean:win32": "del /s /q .\\installers\\win32\\*.* && if exist .\\easybloqs-win32-x64 rmdir /s /q .\\easybloqs-win32-x64",
    "start:electron": "ng build --base-href ./ && electron .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "aws s3 sync .\\dist s3://liquidlogic.eu --delete --profile ll-liquidlogic",
    "package:electron:darwin": "npm install && ng build --prod --base-href ./ && electron-packager . --ignore=/e2e --ignore=/src --ignore=/installers --ignore=/lib/win32 --ignore=/easybloqs-darwin-x64 --overwrite --verbose --asar",
    "package:electron:win32": "npm install && ng build --prod --base-href ./ && electron-packager . --ignore=/e2e --ignore=/src --ignore=/installers --ignore=/lib/darwin --ignore=/easybloqs-win32-x64 --overwrite --verbose --asar --icon=easybloqs-icon.ico",
    "installer:electron:darwin": "npm run clean:darwin && npm run package:electron:darwin && electron-installer-dmg  --overwrite ./easybloqs-darwin-x64/easybloqs.app Easybloqs",
    "installer:electron:win32": "node ./ci/build-installer.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular": "^1.8.1",
    "blockly": "github:leaphy-robotics/leaphy-blockly#3ba98431563a64a4a4d5a35aca14ac074490ff7b",
    "bootstrap": "^4.5.3",
    "electron-prompt": "^1.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "prismjs": "^1.22.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.14.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.64",
    "codelyzer": "^5.2.2",
    "electron": "^8.5.2",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.1",
    "electron-reload": "^1.5.0",
    "electron-winstaller": "^4.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
